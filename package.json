{
  "name": "@sunyfg/table-component",
  "version": "1.0.0",
  "description": "React 表格组件库（TypeScript + Rollup）",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "homepage": "https://github.com/sunyfg/table-component",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunyfg/table-component.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "install:all": "npm install && cd example && npm install",
    "dev": "cd example && npm run dev",
    "build": "rollup -c && sass src/styles/index.scss css/index.css",
    "prepare": "husky install"
  },
  "files": [
    "lib",
    "es",
    "css/*.css"
  ],
  "keywords": [
    "table",
    "react",
    "component",
    "table-component",
    "ui"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.25.6",
    "@eslint/js": "^9.10.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "rollup": "^4.21.2",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.78.0",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
